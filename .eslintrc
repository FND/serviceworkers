root: true
extends: fnd
rules:
    linebreak-style: off
    indent:
    - error
    - 2
    quotes:
    - error
    - single
    keyword-spacing:
    - error
    - before: true
      after: true
      overrides:
          function:
              after: false
    no-var: off
    no-undef: off
    no-console: off
    spaced-comment:
    - error
    - never
